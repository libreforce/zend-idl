{
    "name": "libreforce/zend-idl",
    "description": "provides tools for working with IDLs( Interface description language) and structures",
    "type": "library",
    "require": {
        "php": ">=7.2.6",
        "zendframework/zend-loader": "2.0.*@dev",
        "zendframework/zend-filter": "2.0.*@dev",
        "zendframework/zend-stdlib": "2.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Zend\\Idl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Idl\\": "test/"
        }
    },
